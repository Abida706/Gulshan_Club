<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gulshan_club.Club_Manager.FacilityBookingController">
   <children>
      <Label fx:id="facilitybookingTextfField" layoutX="174.0" layoutY="14.0" prefHeight="37.0" prefWidth="207.0" text="  Facility Booking">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <TableView fx:id="facilityScheduleTableView" layoutX="135.0" layoutY="60.0" prefHeight="200.0" prefWidth="226.0">
        <columns>
          <TableColumn fx:id="facilityNameTableCol" prefWidth="83.0" text="Facility Name" />
          <TableColumn fx:id="bookingDateTablecolumn" prefWidth="91.0" text="Booking Date" />
            <TableColumn fx:id="statusTableColumn" prefWidth="51.0" text="Status" />
        </columns>
      </TableView>
      <Label fx:id="facilityscheduleLabel" layoutX="28.0" layoutY="66.0" text="Facility Schedule" />
      <Button layoutX="135.0" layoutY="269.0" mnemonicParsing="false" onAction="#handleAppro" text="Approve Booking" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="354.0" mnemonicParsing="false" onAction="#handleBackButton" text="Back" />
      <Button fx:id="rejectbookingButton" layoutX="262.0" layoutY="269.0" mnemonicParsing="false" text="Reject Booking" />
   </children>
</AnchorPane>
