<AnchorPane fx:id="memberNameTextField" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gulshan_club.Club_Manager.memberManagementController">
   <children>
      <Label layoutX="180.0" layoutY="14.0" text="Member Management">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <TextField fx:id="userNameTextField" layoutX="168.0" layoutY="71.0" />
      <Label layoutX="32.0" layoutY="75.0" text="User Name" />
      <Label layoutX="376.0" layoutY="75.0" text="Status" />
      <Button layoutX="408.0" layoutY="221.0" mnemonicParsing="false" onAction="#handleAddMemberButton" prefHeight="25.0" prefWidth="121.0" text="Add Member" />
