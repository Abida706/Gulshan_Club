      <Button layoutX="92.0" layoutY="279.0" mnemonicParsing="false" onAction="#handleBackButton" text="Back" />
      <Button layoutX="20.0" layoutY="122.0" mnemonicParsing="false" onAction="#handleVoteButton" text="Vote" />
