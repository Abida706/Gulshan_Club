            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <TextField fx:id="surveyTitleTextField" layoutX="128.0" layoutY="96.0" />
      <Text layoutX="48.0" layoutY="114.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Survey" />
      <Text layoutX="31.0" layoutY="147.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Target Group" />
      <TextField fx:id="targetGroupTextField" layoutX="128.0" layoutY="138.0" />
      <TextArea fx:id="questionTextArea" layoutX="34.0" layoutY="185.0" prefHeight="122.0" prefWidth="265.0" />
