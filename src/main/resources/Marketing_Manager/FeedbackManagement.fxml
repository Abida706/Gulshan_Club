<AnchorPane prefHeight="394.0" prefWidth="788.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gulshan_club.Marketing_Manager.Controller.FeedbackManagementController">
   <children>
      <Label layoutX="235.0" layoutY="32.0" text="Feedback Management" textFill="#0f6e35">
