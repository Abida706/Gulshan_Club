<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gulshan_club.Event_Coordinator.EventFeedbackAnalysisController">
   <children>
      <Label layoutX="190.0" layoutY="14.0" text="Feedback Analysis">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <TableView fx:id="feedbackListTextView" layoutX="14.0" layoutY="62.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="responseIDCol" prefWidth="99.0" text="Response ID" />
          <TableColumn fx:id="feedbackTextCol" prefWidth="99.0" text="feedback Text" />
        </columns>
      </TableView>
      <Button layoutX="224.0" layoutY="237.0" mnemonicParsing="false" onAction="#handleSendFeedbackFormButton" text="Send Feedback Form" />
      <PieChart fx:id="feedbackPieChart" layoutX="372.0" layoutY="92.0" prefHeight="170.0" prefWidth="214.0" />
      <Button layoutX="14.0" layoutY="353.0" mnemonicParsing="false" onAction="#handleBackButton" text="Back" />
      <PieChart layoutX="360.0" layoutY="216.0" prefHeight="141.0" prefWidth="162.0" />
      <Label layoutX="395.0" layoutY="62.0" text="Feedback Summary Chart" />
   </children>
</AnchorPane>
